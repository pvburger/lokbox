{
  "expo": {
    "name": "lokbox",
    "slug": "lokbox",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.pvburger.lokbox"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#d3d3d3"
      },
      "package": "com.pvburger.lokbox"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-sqlite"],
    "extra": {
      "eas": {
        "projectId": "15e47396-788f-4e5f-b787-a875db00d027"
      }
    }
  }
}
